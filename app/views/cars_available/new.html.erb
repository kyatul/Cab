<%= form_for :car_available do |f| %>
  <%= f.label :city %>
  <%= collection_select(:car_available, :city_id, @cities, :id, :name, {:prompt => false}) %><br>

  <%= f.label :car %>
  <%= collection_select(:car_available, :car_id, @cars, :id, :name, {:prompt => false}) %><br>

  <%= f.label :rate_per_km %>
  <%= f.text_field :rate_per_km %><br>

  <%= f.submit "Add Route"%>
<% end %>
